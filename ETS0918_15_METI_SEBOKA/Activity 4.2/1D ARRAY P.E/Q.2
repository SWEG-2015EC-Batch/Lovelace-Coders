#include <iostream>

using namespace std;

int main(){

    
    
    int temp [8] , total = 0;

    for (int i = 0; i < 8; i++){
        cout << "Enter temprature #" << i+1 << ": ";
        cin >> temp[i];
        total+=temp[i];
    }

    cout << "The sum of the temptratures is " << total << '\n';
    cout << "The average of the tempratures is " << total/8.0;
    



    return 0;
    
}
